{
  "name": "book-directory-node",
  "version": "1.0.0",
  "description": "Node.js server for book directory app with JWT Authentication and PostgreSQL db",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "seed:reset": "npx sequelize-cli db:seed:undo:all && npm run migrate",
    "dev-bot": "nodemon ./src/slack-bot.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --testTimeout=10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ese-varo/book-directory-node.git"
  },
  "author": "ese-varo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ese-varo/book-directory-node/issues"
  },
  "homepage": "https://github.com/ese-varo/book-directory-node#readme",
  "dependencies": {
    "@slack/bolt": "^3.3.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules"
    ]
  }
}
